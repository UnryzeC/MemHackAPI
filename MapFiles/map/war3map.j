            if minimap != "" then
                if pMiniMap > 0 then
                    call WriteRealMemory( pReservedIntArg1, 0 )
                    call WriteRealMemory( pReservedIntArg2, 0 )
                    set pTexture = fast_call_3( pTextureLoadImageAnyExt, GetStringAddress( minimap ), pReservedIntArg1, pReservedIntArg2 )

