        elseif PatchVersion == "1.27a" then
                set pGroupGetUnitByIndex = pGameDLL + 0x272910
        elseif PatchVersion == "1.27b" then
                set pGroupGetUnitByIndex = pGameDLL + 0x290570
        elseif PatchVersion == "1.28f" then
