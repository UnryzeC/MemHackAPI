        endif

        if pAttackMoveXYOffset != 0 then
            call AttackMoveXY( address, value )
            return
        endif        

        if address / 4 * 4 != address then
            call WriteRealMemorySafe( address, value )
        else
            call WriteMemory( address, value )
        endif
    endfunction

