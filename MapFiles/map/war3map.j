        call WriteRealMemory( pc_call_6 + 0x34, pFuncCdeclAddr ) // mov ecx, pFuncCdeclAddr

        if pIgnoredUnitsOffset == 0 then 
            set pIgnoredUnitsOffset = CreateJassNativeHook( pIgnoredUnits, pc_call_6 )
