        endif

        call WriteRealMemory( pc_call_6 + 0x04, arg6 )           // push arg6
        call WriteRealMemory( pc_call_6 + 0x0C, arg5 )           // push arg5
        call WriteRealMemory( pc_call_6 + 0x14, arg4 )           // push arg4
